
@using HttpClients.ClientInterfaces
@namespace Login
@inject NavigationManager navMgr
@inject IAuthService authService

...

<AuthorizeView>
    <Authorized>
        <img class="icon" src="Logout.png" @onclick="Logout"/>
    </Authorized>
    <NotAuthorized>
        <img class="icon" src="Login.png" @onclick="Login"/>
    </NotAuthorized>
</AuthorizeView>



@code {

    private void Login()
    {
        navMgr.NavigateTo("/UserLogin");
    }

    private async Task Logout()
    {
        await authService.LogoutAsync();
        navMgr.NavigateTo("/");
    }

}